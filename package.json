    "@anycli/test": "^0.10.2",
    "@anycli/tslint": "^0.2.2",
    "@anycli/version": "^0.1.19",
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.100",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^9.1.2",
    "@types/node": "^9.4.0",
    "@types/node-notifier": "^0.0.28",
